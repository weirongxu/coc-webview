{
  "name": "coc-webview",
  "version": "0.1.0",
  "description": "Vscode webview ported to coc.nvim",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "node esbuild.js --watch",
    "build:type": "tsc --emitDeclarationOnly",
    "build:pack": "node esbuild.js",
    "build": "run-s build:type lint build:pack",
    "prepare": "run-s clean build"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.7.4",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "coc-helper": "^0.11.1",
    "coc.nvim": "^0.0.81-next.6",
    "esbuild": "^0.12.24",
    "esbuild-plugin-alias": "^0.1.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mime-types": "^2.1.32",
    "npm-run-all": "^4.1.5",
    "open": "^8.2.1",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "type-fest": "^2.1.0",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Markdown Preview Enhanced",
      "properties": {
        "webview.debug": {
          "type": "boolean",
          "default": "false"
        },
        "webview.host": {
          "type": "string",
          "default": "localhost"
        },
        "webview.minPort": {
          "type": "number",
          "default": "15567"
        },
        "webview.maxPort": {
          "type": "number",
          "default": "15667"
        }
      }
    }
  }
}
